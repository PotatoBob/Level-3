import java.awt.Graphics;

import javax.swing.JFrame;
import javax.swing.JPanel;

public class OpticalIllusion extends JPanel {
	int col = 4;
	int row = 4;
	JFrame window;
	int[][] grid = new int[col][row];
	public static void main(String[] args) {
		
		OpticalIllusion OI = new OpticalIllusion();
	}
	public void paint(Graphics g) {
		for (int i = 0; i < col; i++) {
			for (int j = 0; j < row; j++) {
				g.fillRect(50*i, 50*j, 50, 50);
			}
		}
	}
	OpticalIllusion() {
		window = new JFrame();
		
	}
}
