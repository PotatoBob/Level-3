import javax.swing.JButton;
import javax.swing.JFrame;
import javax.swing.JPanel;

public class WordScramble {
	JFrame window;
	JPanel panel;
	JButton add;
	JButton view;
	JButton play;
	public static void main(String[] args) {
		WordScramble WS = new WordScramble();
	}
	WordScramble() {
		window = new JFrame();
		window.setVisible(true);
		
		panel = new JPanel();
		
		window.add(panel);
	}
}
