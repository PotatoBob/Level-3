import org.junit.Test;

public class AnimalHipsters {
	ArrayList<String> Amy = new ArrayList<>();
	ArrayList<String> Brie = new ArrayList<>();	
	ArrayList<String> Chris = new ArrayList<>();	
	ArrayList<String> Devney = new ArrayList<>();	
	ArrayList<String> Elena = new ArrayList<>();		
	ArrayList<String> FanHal = new ArrayList<>();
	public static void main(String[] args) {
		AnimalHipsters AH = new AnimalHipsters();
	}
	AnimalHipsters() {
		Amy.add("Brie");
		Amy.add("Chris");
		Brie.add("Amy");
		Brie.add("Chris");
		Brie.add("Devney");
		Brie.add("Elena");
		Chris.add("Amy");
		Chris.add("Brie");
		Chris.add("Elena");
		Chris.add("Fan-Hal");
		Devney.add("Brie");
		Devney.add("Elena");
		Elena.add("Brie");
		Elena.add("Chris");
		Elena.add("Devney");
		Elena.add("Fan-Hal");
		FanHal.add("Chris");
		FanHal.add("Elena");
	}
	private ArrayList<String> findAnimalHipsters(HashMap<String, ArrayList<String>> network, HashMap<String, String> favoriteAnimals) {
		network.put("Amy", Amy);
		network.put("Brie", Brie);
		network.put("Chris", Chris);
		network.put("Devney", Devney);
		network.put("Elena", Elena);
		network.put("Fan-Hal", FanHal);
		
		favoriteAnimals.put("Amy", "Quokka");
		favoriteAnimals.put("Brie", "Springbok");
		favoriteAnimals.put("Chris", "Nubian Ibex");
		favoriteAnimals.put("Devney", "Springbok");
		favoriteAnimals.put("Elena", "Quokka");
		favoriteAnimals.put("Fan-Hal", "Springbok");
		return null;
	}	
}
