import static org.junit.Assert.*;

import org.junit.Test;

public class FibonacciTest {

	@Test
	public void test() {
		assertEquals(2, factorial(2));
		assertEquals(6, factorial(3));
		assertEquals(24, factorial(4));
		assertEquals(120, factorial(5));
	}
	int factorial(int x) {
		int y = 1;
		if (x == 1) {
			y = 1;
		} else {
			for (int i = 1; i <= x; i++) {
				y=y*i;
			}
		}
		return y;
	}

}
